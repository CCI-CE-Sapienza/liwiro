<p id="notice"><%= notice %></p>

<h1>my Gyms</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Likes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%  (@gyms || []).each do |gym| %>
      <tr>
        <td><%= gym.email %></td>
        <td><%= gym.likes %></td>
        <td><%= link_to 'Show', gym %></td>
        <td><%= link_to 'Leave' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
