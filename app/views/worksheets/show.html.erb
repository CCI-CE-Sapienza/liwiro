<br>
<h1>Your Worksheet</h1>

<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Comments</th>
      <th scope="col">Athlete</th>
      <th scope="col">Gym</th>

    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @worksheet.comments %></td>
        <td><%= @worksheet.athlete.username %></td>
        <td><%= @worksheet.gym.username %></td>
      </tr>
  </tbody>
</table>

<%= link_to "<button type='button' class='btn btn-primary navbar-btn' data-toggle='modal' data-target=''>Edit</button>".html_safe, edit_worksheet_path(@worksheet)%>

<%= link_to "<button type='button' class='btn btn-primary navbar-btn' data-toggle='modal' data-target=''>Back</button>".html_safe, worksheets_path %>
