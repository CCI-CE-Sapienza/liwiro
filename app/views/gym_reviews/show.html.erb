<h1>Your Review</h1>
<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Stars</th>
      <th scope="col">Text</th>
      <th scope="col">Athlete</th>
      <th scope="col">Gym</th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @gym_review.stars %></td>
        <td><%= @gym_review.text %></td>
        <td><%= @gym_review.athlete.username %></td>
        <td><%= @gym_review.gym.username %></td>
      </tr>
  </tbody>
</table>
